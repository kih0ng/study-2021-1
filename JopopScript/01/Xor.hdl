/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=outa);

    Nand(a=a, b=a, out=outb);
    Nand(a=b, b=b, out=outc);
    Nand(a=outb, b=outc, out=outd);

    Nand(a=outa, b=outd, out=oute);
    Nand(a=oute, b=oute, out=out);
}